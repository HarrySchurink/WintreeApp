<?xml version="1.0" encoding="UTF-8" ?>
    <widget xmlns = "http://www.w3.org/ns/widgets"
        xmlns:gap = "http://phonegap.com/ns/1.0"
        id        = "com.tsd.tsdapp"
        versionCode="10" 
        version   = "1.0.0">

    <!-- versionCode is optional and Android only -->

    <name>TSDApp</name>

    <description>
        TSD APP test fabriek
    </description>

    <author href="http://www.tsd.nl" email="h.schurink@tsd.nl">
        Harry Schurink
    </author>

    <gap:plugin name="ChildBrowser"/>
